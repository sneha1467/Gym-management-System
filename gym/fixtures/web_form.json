  "client_script": "console.log(\"Hello World\");\n\ndocument.getElementsByClassName(\"nav-avatar\")[0].style.display = \"none\";\ndocument.getElementsByClassName(\"navbar-brand\")[0].style.display = \"none\";\n\n\nwindow.onload = function() {\n\n    document.getElementsByClassName(\"nav-avatar\")[0].style.display = \"none\";\n    document.getElementsByClassName(\"navbar-brand\")[0].style.display = \"none\";\n};\n    \nlet value = localStorage.getItem('email');\nconst emailField = document.querySelector('input[data-fieldname=\"email\"]');\nemailField.value=value;\nlet email = value;\nemailField.setAttribute(\"readonly\", true);\n\n\n frappe.call({\n        method: 'frappe.client.get',\n        args: {\n            doctype: 'Gym Class Booking',\n            name: email  \n        },\n        callback: function(res) {\n            let booking = res.message;\n            console.log(\"Gym Class Booking response:\", booking);\n\n            if (booking && booking.trainer_name) {\n                let trainer_name = booking.trainer_name;\n                console.log(\"Trainer name from booking:\", trainer_name);\n                setTrainerFieldOptions([trainer_name], trainer_name);\n            } else {\n                setTrainerFieldOptions([], '');\n            }\n        }\n    });\n\nfunction setTrainerFieldOptions(options, value_to_set) {\n\n    frappe.web_form.set_df_property('trainer_name', 'options', options);\n\n\n    let field = frappe.web_form.get_field('trainer_name');\n    if (field && field.df) {\n        field.df.options = options;\n    }\n\n\n    frappe.web_form.set_value('trainer_name', value_to_set);\n\n \n    if (field && field.refresh) {\n        field.refresh();\n    }\n}\n",
